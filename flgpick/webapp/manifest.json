{
  "_version": "1.65.0",
  "sap.app": {
    "id": "ns.flgpick",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.1",
      "toolsId": "4b7b1b80-e0cb-4761-ae4f-2f16025fd7fa"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/YY1_FLGPCK_API_CDS/",
        "type": "OData",
        "settings": {
          "annotations": [
            "annotationMain"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "annotationMain": {
        "type": "ODataAnnotation",
        "uri": "annotations/annotationMain.xml",
        "settings": {
          "localUri": "annotations/annotationMain.xml"
        }
      },
      "warehouseresource": {
        "uri": "/sap/opu/odata4/sap/api_warehouse_resource_2/srvd_a2x/sap/warehouseresource/0001/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/warehouseresource/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "warehouseorder": {
        "uri": "/sap/opu/odata4/sap/api_warehouse_order_task_2/srvd_a2x/sap/warehouseorder/0001/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/warehouseorder/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "pickpack-display": {
          "semanticObject": "pickpack",
          "action": "display",
          "title": "{{pickpack-display.flpTitle}}",
          "subTitle": "{{pickpack-display.flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.130.11",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ns.flgpick.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "warehouseresource": {
        "dataSource": "warehouseresource",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "warehouseorder": {
        "dataSource": "warehouseorder",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "ns.flgpick.view",
        "async": true,
        "viewPath": "ns.flgpick.view"
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        }
      ],
      "targets": {
        "TargetMain": {
          "id": "Main",
          "name": "Main"
        }
      }
    },
    "rootView": {
      "viewName": "ns.flgpick.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "flgpick"
  }
}
